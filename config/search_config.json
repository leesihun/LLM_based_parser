{
  "web_search": {
    "enabled": true,
    "timeout": 15,
    "max_results": 5,
    "delay": 2,
    "search_engine": "google",
    "selenium": {
      "headless": true,
      "browser_preference": ["chrome", "firefox"],
      "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
    },
    "keyword_extraction": {
      "enabled": true,
      "extraction_methods": [
        "rule_based",
        "tfidf",
        "llm_assisted"
      ],
      "max_keywords": 8,
      "min_keyword_length": 2,
      "query_expansion": true,
      "require_adequate_keywords": true,
      "technical_terms_boost": 2.0,
      "multiword_terms_boost": 1.5
    }
  }
}